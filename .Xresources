
! base
URxvt.font: xft:monospace-7
URxvt.letterSpace: -2
URxvt.cursorBlink: true
URxvt.pointerBlank: true
URxvt.fading: 40
URxvt.depth: 32
URxvt.saveLines: 10000
URxvt*shading: 5
URxvt.iso14755: false
URxvt.iso14755_52: false

! clickable url
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.keysym.C-Delete: perl:matcher:last
URxvt.keysym.M-Delete: perl:matcher:list
URxvt.perl-ext: default,url-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true

! scroll back
URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true
!URxvt.secondaryScreen: 1
!URxvt.secondaryScroll: 0

! scroll bar
URxvt.scrollBar: true
URxvt*scrollBar_right: true
URxvt.scrollBar_floating: true
URxvt*scrollstyle: plain

! key
URxvt.keysym.C-S-Left:		string:\033[1;6D
URxvt.keysym.C-S-Right:		string:\033[1;6C
URxvt.keysym.C-Up:          string:\033[1;5A
URxvt.keysym.C-Down:        string:\033[1;5B
URxvt.keysym.C-Left:        string:\033[1;5D
URxvt.keysym.C-Right:       string:\033[1;5C

