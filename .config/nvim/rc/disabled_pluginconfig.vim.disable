     function! FloatingFZF()
       let width = float2nr(&columns * 0.9)
       let height = float2nr(&lines * 0.6)
       let opts = {
             \ 'relative': 'editor',
             \ 'row': (&lines - height) / 2,
             \ 'col': (&columns - width) / 2,
             \ 'width': width,
             \ 'height': height,
             \ 'style': 'minimal'
