let g:multi_cursor_use_default_mapping=0
nnoremap <multiple-cursors>   <Nop>
nmap    R <multiple-cursors>
let g:multi_cursor_start_word_key      = 'R'
let g:multi_cursor_select_all_word_key = '<Leader>R'
let g:multi_cursor_start_key           = 'gR'
let g:multi_cursor_select_all_key      = 'g<Leader>R'
let g:multi_cursor_next_key            = '<C-n>'
let g:multi_cursor_prev_key            = '<C-p>'
let g:multi_cursor_skip_key            = '<C-x>'
let g:multi_cursor_quit_key            = '<Esc>'
