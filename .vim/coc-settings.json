{
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "diagnostic-languageserver.filetypes": {
    "vim": "vint",
    "markdown": [ "write-good", "markdownlint" ],
    "sh": "shellcheck",
  },
  "list.insertMappings": {
    "<C-v>": "action:vsplit",
    "<C-s>": "action:split",
    "<C-w>": "command:wincmd k"
  },
  "rust-analyzer.cargo-watch.enable": true,
  "rust-analyzer.cargo-watch.command": "clippy",
  "rust-analyzer.cargo-watch.allTargets": true,
  "python.jediEnabled": false,
  "emmet.includeLanguages": {"vue-html": "html", "javascript": "javascriptreact"},
  "yank.enableCompletion": false,
  "snippets.userSnippetsDirectory": "~/.vim/snippets/",
  "snippets.snipmate.enable": false,
  "snippets.convertToSnippetsAction": false,
  "explorer.icon.enableNerdfont": true,
  "codeLens.enable": true,
  "tabnine.priority": 50,
  "cSpell.showStatus": false,
  "translator.toLang": "ja",
  "translator.engines": [
    "google",
    "bing"
  ]
}
