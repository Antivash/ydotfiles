{
  "diagnostic-languageserver.filetypes": {
    "vim": "vint",
    "markdown": [ "write-good", "markdownlint" ],
    "sh": "shellcheck",
  },
  "rust-analyzer.cargo-watch.enable": true,
  "rust-analyzer.cargo-watch.command": "clippy",
  "rust-analyzer.cargo-watch.allTargets": true,
  "python.jediEnabled": false,
  "emmet.includeLanguages": {"vue-html": "html", "javascript": "javascriptreact"},
  "yank.enableCompletion": false,
  "snippets.userSnippetsDirectory": "~/.vim/snippets/",
  "snippets.snipmate.enable": false,
  "codeLens.enable": true,
  "tabnine.priority": 50,
  "cSpell.showStatus": false,
  "translator.toLang": "ja",
  "translator.engines": [
    "google",
    "bing"
  ],
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "go",
    "handlebars",
    "haskell",
    "html",
    "jade",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "pug",
    "python",
    "restructuredtext",
    "rust",
    "scala",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "vim",
    "yaml",
    "yml"
  ]
}
